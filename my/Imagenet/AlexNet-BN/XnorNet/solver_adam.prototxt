net: "train_test.prototxt" 
test_iter: 1000 
test_interval: 1000 
base_lr: 0.001
momentum: 0.9
momentum2: 0.999
# since Adam dynamically changes the learning rate, we set the base learning
# rate to a fixed value
lr_policy: "fixed"
# Display every 100 iterations
display: 200 
max_iter: 480000 
snapshot: 10000
snapshot_prefix: "snapshot" 
type: "Adam"
solver_mode: GPU